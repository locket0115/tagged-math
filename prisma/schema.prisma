generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Problem {
  id          Int      @id @default(autoincrement())
  description String
  tags        String[]
  imageUrl    String?
}

model Tag {
  id   Int    @id @default(autoincrement())
  name String @unique
}